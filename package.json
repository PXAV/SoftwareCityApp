  "build": {
    "appId": "org.software-city.app.desktopapp",
    "linux": {
      "target": "deb",
      "icon": "D:\\Bibliotheken\\Desktop\\Workspace\\SWC\\SWC_desktopapp\\dist-resources\\logo.png",
      "publish": {
        "provider": "github",
        "publishAutoUpdate": true,
        "releaseType": "release"
      }
    },
    "win": {
      "target": "nsis",
      "icon": "D:\\Bibliotheken\\Desktop\\Workspace\\SWC\\SWC_desktopapp\\dist-resources\\logo.ico",
